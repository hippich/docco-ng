{
  "name": "docco-ng",
  "description": "Generated static project documentation primarily for node.js projects. A fork of Docco-husky modified to use marked instead of showdown and prettyprint instead of pygments",
  "keywords": [
    "documentation",
    "docs",
    "generator"
  ],
  "author": "Pavel Karoukin <pavel@karoukin.us>",
  "version": "0.1.0",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/mit-license.php"
    }
  ],
  "contributors": [
    "Pavel Karoukin <pavel@karoukin.us>",
    "Mike Brevoort <mike@brevoort.com>",
    "Jeremy Ashkenas"
  ],
  "engines": {
    "node": ">=0.4.0"
  },
  "bugs": {
    "url": "https://github.com/hippich/docco-ng/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hippich/docco-ng.git"
  },
  "dependencies": {
    "dox": "https://github.com/royriojas/dox/tarball/master",
    "jade": "0.17.0",
    "gravatar": "1.0.3",
    "underscore": "1.2.x",
    "escape-html": "~1.0.1",
    "marked": "~0.3.1",
    "glob-expand": "0.0.2"
  },
  "directories": {
    "lib": "./lib"
  },
  "main": "./lib/docco",
  "bin": {
    "docco-husky": "./bin/generate"
  },
  "optionalDependencies": {},
  "docco_husky": {
    "content_dir": "md",
    "output_dir": "./documentation",
    "project_name": "docco-husky",
    "show_timestamp": true
  },
  "devDependencies": {
    "build-workflow": "0.0.21",
    "grunt": "~0.4.5",
    "grunt-bump": "0.0.15",
    "grunt-ez-frontend": "~0.2.21",
    "es6-promise": "~1.0.0",
    "time-grunt": "~1.0.0",
    "matchdep": "~0.3.0"
  }
}
